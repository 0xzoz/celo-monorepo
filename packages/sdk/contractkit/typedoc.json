{
  "mode": "modules",
  "exclude": [
    "**/generated/*.ts",
    "**/*+(index|.test).ts"
  ],
  "excludeNotExported": true,
  "excludePrivate": true,
  "excludeProtected": true,
  "includeDeclarations": false,
  "ignoreCompilerErrors": true,
  "hideGenerator": "true",
  "out": "./docs",
  "gitRevision": "contractkit-v1.2.2",
  "plugins": [
    [
      "docusaurus-plugin-typedoc",
      {
        "entryPoints": [
          "./src/index.ts"
        ],
        "tsconfig": "./tsconfig.json"
      }
    ]
  ]
}